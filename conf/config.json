                "rule_type": "1",
                "hs_flag": "LEFTMOST"
            },
            {
                "id": "401",
                "mz": "$response_body",
                "rx": "[^0-9]1(3[0-9]|4[57]|5[0-35-9]|7[0135678]|8[0-9])\\d{8}[^0-9]",
