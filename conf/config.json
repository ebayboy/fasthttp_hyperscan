{
    "debug":true,
    "version": "v1.0.0",
    "cpunum": 1,
    "procnum": 1,
    "hsconfig": {
        "debug":"1",
        "rules": [
            {
                "id": "10001",
                "mz": "$request",
                "rx": "request "
            },
            {
                "id": "10002",
                "mz": "$request_body",
                "rx": "request_body"
            },
            {
                "id": "12011",
                "mz": "$request,$request_body,$request_uri,$http_user_agent,$http_referer,$http_cookie,$uri,$arg_name",
                "rx": "request|request_body|request_uri|http_user_agent|http_referer|http_cookie|uri|arg_name"
            }
        ],
        "policies": [
            {
                "id": "10001",
                "policy": "10001"
            },
            {
                "id": "10002",
                "policy": "10002"
            },
            {
                "id": "10003",
                "policy": "10003"
            },
            {
                "id": "10000",
                "policy": "10001&10002&10003"
            }
        ]
    }
}
