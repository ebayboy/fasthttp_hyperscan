                "mz": "$response_body",
                "rx": "[^0-9](\\d{16}|\\d{17}|\\d{19})[^0-9]",
                "rule_type": "1",
                "hs_flag": "LEFTMOST"
            },
            {
                "id": "403",
                "mz": "$response_body",
                "rx": "[^0-9][A-Za-z0-9._+-]+@[a-zA-Z0-9_-]+\\.com(\\.cn)?[^0-9]",
